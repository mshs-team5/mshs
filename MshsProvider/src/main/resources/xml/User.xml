<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.szxs.dao.TUserDao">

    <select id="getUser" resultType="TUsers" >
        select * from t_users
        where CARD_ID=#{id}
    </select>
    <select id="queryAll" resultType="TUsers">
        select * from t_users
    </select>
</mapper>